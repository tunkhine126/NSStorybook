{
  "name": "cypress-graphql-mock",
  "description": "Mock out a GraphQL schema from the client",
  "version": "0.2.0",
  "scripts": {
    "dev": "tsc -w",
    "prepublish": "tsc"
  },
  "main": "dist",
  "types": "src",
  "files": [
    "dist/",
    "src/",
    "yarn.lock",
    "README.md"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "author": {
    "name": "Tim Griesser"
  },
  "dependencies": {
    "graphql-tools": "^4.0.4",
    "tslib": "^1.9.3"
  },
  "peerDependencies": {
    "cypress": "^3.1.3",
    "graphql": "^14.1.1"
  },
  "devDependencies": {
    "@types/graphql": "^14.0.3",
    "cypress": "^3.1.3",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "typescript": "^3.2.2"
  }
}
